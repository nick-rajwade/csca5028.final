@page "/speeddial-features"
@using Syncfusion.Blazor.Buttons
<PageTitle>Speed Dial</PageTitle>
<h2>Speed Dial</h2>
<br/>
<div id = "ControlRegion">
<div class="control-section">
 <div id="target" style="position:relative; border:1px solid black;height:300px" ></div>
<SfSpeedDial Mode=SpeedDialMode.Linear Target="#target" IsModal=true OpenIconCss="speeddial-icons speeddial-icon-edit" Position=Syncfusion.Blazor.Buttons.FabPosition.TopLeft >    
	<SpeedDialItems>
		<SpeedDialItem IconCss="speeddial-icons speeddial-icon-cut"></SpeedDialItem>
		<SpeedDialItem IconCss="speeddial-icons speeddial-icon-copy"></SpeedDialItem>
		<SpeedDialItem IconCss="speeddial-icons speeddial-icon-paste"></SpeedDialItem>
		<SpeedDialItem IconCss="speeddial-icons speeddial-icon-delete"></SpeedDialItem>
		<SpeedDialItem IconCss="speeddial-icons speeddial-icon-save"></SpeedDialItem>
	</SpeedDialItems>
</SfSpeedDial>
</div>
</div>
<br/>
<div>
  <h3>Selected Features:</h3>
    <ul class="ulstyle">
      <li class="list"> Type - Linear</li>
      <li class="list"> Position - TopLeft</li>
      <li class="list"> Modal</li>
      <li class="list"> Theme - Material</li>
    </ul>
</div>
<br/>
